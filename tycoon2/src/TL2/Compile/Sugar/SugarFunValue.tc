class SugarFunValue
super SugarValue
metaclass SugarFunValueClass
public

srcFun :String,
srcParenOpen :String,
domain :String,
srcParenClose :String,
range :String,
body :SugarList(SugarSequenceElement)

methods

asFun :SugarFunValue
{ self }

