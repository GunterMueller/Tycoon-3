class SubTypeIdeClass
super ConcreteClass(SubTypeIde)
metaclass MetaClass
public methods
newSig(name :Positioned(String), p :SourcePos, type :Type) :SubTypeIde
{
  name.isNil ? {
    new1(p, nil, type)
  } : {
    new1(name.pos, name.value, type)
  }
}

new1(pos :SourcePos, name :String, type :Type) :SubTypeIde
{
  let instance = _new,
  instance.pos := pos,
  instance.name := name,
  instance.type := type,
  instance
}
;
