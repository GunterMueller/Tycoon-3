class TypeIdeClass
super ConcreteClass(TypeIde)
metaclass MetaClass
public methods
newSig(name :Positioned(String), p :SourcePos, type :Type) :TypeIde
{
  name.isNil ? {
    new1(p, nil, type)
  } : {
    new1(name.pos, name.value, type)
  }
}

newRef(name :Positioned(String)) :TypeIde
{
  new1(name.pos, name.value, nil)
}

new1(pos :SourcePos, name :String, type :Type) :TypeIde
{
  let instance = _new,
  instance.pos := pos,
  instance.name := name,
  instance.type := type,
  instance
}
;
