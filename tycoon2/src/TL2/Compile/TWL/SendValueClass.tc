class SendValueClass
super ConcreteClass(SendValue)
metaclass MetaClass
public methods
new(receiver :Value, selector :Symbol, args :List(Value)) :SendValue
{
  new1(receiver.pos, receiver, selector, args)
}

new1(pos :SourcePos, 
     receiver :Value, selector :Symbol, args :List(Value)) :SendValue
{
  let instance = _new,
  instance.pos := pos,
  instance.receiver := receiver,
  instance.selector := selector,
  instance.args := args,
  instance
}
;
