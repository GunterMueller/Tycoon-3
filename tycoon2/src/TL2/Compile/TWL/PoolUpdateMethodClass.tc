class PoolUpdateMethodClass
super CStructClass(PoolUpdateMethod)
metaclass MetaClass
public methods
new( pos :SourcePos, selector :Symbol,
     slotType :Type, cell :Cell(Object)) :PoolUpdateMethod
{
  let instance = _new,
  instance.pos := pos,
  instance.selector := selector,
  instance.methodType :=
     MethodType.new1( pos, selector, 
		     List.with1(ValueIde.new1(pos, nil, slotType)), 
		     slotType),
  instance.cell := cell,
  instance.arity := 1,
  instance._init
}
;
