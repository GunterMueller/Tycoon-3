class SlotAccessMethodClass
super CStructClass(SlotAccessMethod)
metaclass MetaClass
public methods
new( pos :SourcePos, selector :Symbol,
     slotType :Type, isPrivate :Bool) :SlotAccessMethod
{
  let instance = _new,
  instance.pos := pos,
  instance.selector := selector,
  instance.methodType := 
    MethodType.new1( pos, selector, EmptyList.new, slotType),
  instance.isPrivate := isPrivate,
  instance._init
}
;
