class LocalVarClass
super ConcreteClass(LocalVar)
metaclass MetaClass
public methods
new(ide :ValueIde, idx :Int) :LocalVar
{
  assert idx >= 0,
  idx > 255 ? {
    ImplementationRestriction.new(
      "frame offset for local variable ("+ide.printString+")",
      255, idx).raise
  },
  let instance = _new,
  instance.ide := ide,
  instance.idx := idx,
  instance._init
}
;
