class GlobalVarClass
super ConcreteClass(GlobalVar)
metaclass MetaClass
public methods
new(ide :ValueIde, idx :Int) :GlobalVar
{
  assert idx >= 0,
  idx > 255 ? {
    ImplementationRestriction.new(
      "index for function global variable ("+ide.printString+")",
      255, idx).raise
  },
  let instance = _new,
  instance.ide := ide,
  instance.idx := idx,
  instance._init
}
;
