class ImplementationRestriction
super BackEnd, Exception
public

what :String,
maximum :Int,
found :Int

methods

printOn(out :Output)
{ out.writeString("Implementation restriction: "),
  out.writeString(what),
  out.writeString(" restricted to "),
  maximum.printOn(out),
  out.writeString(", encountered "),
  found.printOn(out),
  out.nl
}
;



