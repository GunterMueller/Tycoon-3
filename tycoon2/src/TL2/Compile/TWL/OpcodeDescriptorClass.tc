class OpcodeDescriptorClass
super ConcreteClass(OpcodeDescriptor)
metaclass MetaClass
public methods
new(n :Int, name :String, argBytes :Int) :OpcodeDescriptor
  require 0 <= n & n < 256
{
  assert 0 <= n & n < 256,
  let instance = _new,
  instance.n := n,
  instance.name := name,
  instance.argBytes := argBytes,
  instance._init
}
;
