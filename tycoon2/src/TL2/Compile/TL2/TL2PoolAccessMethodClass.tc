meta MetaClass
{
new(pos :SourcePos, selector :Symbol, domain :List(TL2Ide), range :TL2Type, 
				  cell :Cell(Object)) :TL2PoolAccessMethod
  (* *)
{
  assert({ domain.all(fun(i:TL2Ide){ !i.isValueIde }) })
  instance ::= _new
  instance.pos := pos
  instance.selector := selector
  instance.methodType := TL2MethodType.new(pos, selector, domain, range)
  instance.cell := cell
  instance.arity := 0  ; ### better: actually scope this method! 
  instance._init
  instance
}

}
