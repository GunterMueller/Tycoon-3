      <h1>define-test</h1>

	<define name=hans type='String'> lala </define>
	<ref name=hans//
<p>
	<define name=f>
	  <fun param="s :String, y :Fun(:String):String">
	    <apply name=y><arg><ref name=s//</apply>
	  </fun>
	</define>

	<define name=ignore>
	  <fun param=":String">ignoriert<br></fun>
	</define>

	<apply name=ignore><arg/hallo/</apply>

	<apply name=f>
	  <arg name=s/Hallo /
	  <arg name=y>
	    <fun param="s1 :String"><ref name=s1//Kleines</fun>
	  </arg>
	</apply>

