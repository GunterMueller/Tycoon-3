class AbstractArrayDescriptor
super TspDescriptor
metaclass AbstractClass
public methods

objectSize(o :Object, tracer :Tracer) :Int
{ o.__basicSize * tracer.sizeof(elementFmt) }

elementFmt :Char
  deferred

dumpInstance(o :Object, tracer :Tracer)
{
  for(0,o.__basicSize-1,fun(i:Int){
    tracer.dumpC(o.__basicAt(i), elementFmt)
  })
}
;
