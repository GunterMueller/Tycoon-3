meta X_TL2ConsSubstClass
{
ide :X_TL2Ide

type :X_TL2Type

tail :X_TL2Subst

subst(T <:Void,
      ide :X_TL2Ide,
      ifNotFound :Fun():T,
      cont :Fun(:X_TL2Type, :X_TL2Subst):T) :T
  (* *)
{
  if( ide == self.ide
    then: { cont[type, tail] }
    else: { tail.subst(ide, ifNotFound, cont) })
}

printOn(o :Output) :Void
  (* *)
{
  o << "[" << type << " / " << ide.name << "[" << ide.identityHash << "]]" << tail
}

}
